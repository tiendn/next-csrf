    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.1",
    "next": "^9.5.5",
    "np": "^6.5.0",
    "prettier": "^2.1.2",
    "rollup": "^2.32.1",
    "rollup-plugin-analyzer": "^3.3.0",
